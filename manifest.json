{
  "manifest_version": 2,
  "name": "Discord RPC Pack",
  "version": "0.0.1",
  "description": "",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": ["extensionId.js", "Soundcloud/main.js"],
      "matches": ["https://soundcloud.com/*"]
    },
    {
      "js": ["extensionId.js", "Youtube/main.js"],
      "matches": ["https://www.youtube.com/*"]
    }
  ]
}
